<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>
  <servlet>
    <servlet-name>jsp</servlet-name>
    <servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
    <init-param>
      <param-name>jspCompilerPlugin</param-name>
      <param-value>com.borland.jbuilder.webserverglue.tomcat.jsp.JasperSunJavaCompiler</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>shtml</servlet-name>
    <servlet-class>com.borland.jbuilder.webserverglue.shtml.ShtmlLoaderEcho</servlet-class>
    <init-param>
      <param-name>docBase</param-name>
      <param-value>F:/eschool/jsproot</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>studenthomeworkdownload</servlet-name>
    <servlet-class>com.dc.eschool.web.DownloadHomeWorkServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jsp</servlet-name>
    <url-pattern>*.jsp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>shtml</servlet-name>
    <url-pattern>*.shtml</url-pattern>
  </servlet-mapping>
</web-app>
